<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <!--Modificacion del formulario de proyecto-->
    <record id="fiaes_account_form" model="ir.ui.view">
      <field name="name">fiaes project form</field>
      <field name="model">project.project</field>
      <field name="inherit_id" ref="project.project_project_view_form_simplified" />
      <field name="arch" type="xml">
        <xpath expr="//field[@name='name']" position="after">
          <field name="fuentes_ids" string='Fuentes de Financiamiento'/>
        </xpath>
        <xpath expr="//field[@name='fuentes_ids']" position="attributes">
          <attribute name="widget">many2many_tags</attribute>
        </xpath>
      </field>
    </record>
    
  </data>
</odoo>
